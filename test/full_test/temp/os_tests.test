os_list = [
            ("ubuntu", "build/debug/rethinkdb"),
            ("redhat5_1", "build/debug-legacy-legacy-gcc-no_eventfd/rethinkdb"),
            ("debian", "build/debug-legacy-no_eventfd/rethinkdb"),
            ("centos5_5", "build/debug-legacy-legacy-gcc/rethinkdb"),
            ("suse", "build/debug-legacy-legacy-gcc-no_eventfd/rethinkdb"),
            ("centos6", "build/debug-legacy-legacy-gcc/rethinkdb")
            ]

workload = "./rethinkdb/test/workloads/many_keys.py $HOST:$PORT"

for os_name, build_folder in os_list:
    do_test(
        "$RETHINKDB/scripts/VirtuaBuild/vm_access.py --vm-name %s --command ./rethinkdb/test/scenarios/static_cluster.py '%s'" % (os_name, workload),
        repeat = 3,
        inputs = [build_folder, "test/scenarios", "test/common", "scripts/VirtuaBuild", "test/workloads"]
        )
