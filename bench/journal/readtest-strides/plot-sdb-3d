#-*- gnuplot -*-

set terminal png
set lmargin 5
set output "graph-sdb.png"

set title 'Block size, stride, and ops/sec on a solid-state drive'
set xlabel 'Stride'
set ylabel 'Block Size'
set zlabel 'Ops/Sec'

set ytics 512,512
set xtics 512,512
set pm3d at b

splot 'results' using 3:2:4 every :::8 with lines title ""

