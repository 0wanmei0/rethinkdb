#-*- gnuplot -*-

set terminal png size 1000 800
set rmargin 3
set output "graph-sda.png"

set title 'Block size vs. ops/sec on a rotational drive'
set xlabel 'Block size'
set ylabel 'ops/sec'
set xtics 512,512
set yr [80:140]
set grid

plot 'results' using 1:2 title 'Block size vs. ops/sec (uniform)' with lines,\
     'results' using 1:3 title 'Block size vs. ops/sec (normal)' with lines,\
     'results' using 1:4 title 'Block size vs. ops/ses (pow)' with lines
