echo "Duration: $CANONICAL_DURATION"
echo "Stress client location: $STRESS_CLIENT"
echo "$CANONICAL_CLIENTS concurrent clients"
echo "Server hosts: $SERVER_HOSTS"
if [ $DATABASE == "rethinkdb" ]; then
    echo "Server parameters: --active-data-extents 1 -c 12 -m 256 $SSD_DRIVES"
elif [ $DATABASE == "mysql" ]; then
    echo "Server parameters: $MYSQL_COMMON_FLAGS --innodb_buffer_pool_size=256m $MYSQL_DURABILITY_FLAGS $MYSQL_SSD_FLAGS"
elif [ $DATABASE == "membase" ]; then
    echo "Server parameters: -d $MEMBASE_DATA_PATH -m 256"
fi
